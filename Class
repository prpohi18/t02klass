public class lesson{
  public static void main(String[] args){


    aircraft lennart = new aircraft(); //creates object of class aircraft

    double meriEndurance;
    lennart.seats = 170;
    lennart.passengers = 150;
    lennart.cruiseSpeed = 500;
    lennart.fuelCapacity = 200;
    lennart.fuelBurnRate = 10;

    System.out.println("Lennuk lennart: ");
    System.out.println();
    System.out.println("Lennuki vastupidavus on: "+lennart.calculateEndurance()+" tundi");
    System.out.println("Lennuki kiirus on: "+lennart.cruiseSpeed+"km/h");
    System.out.println("Lennuk mahutab: "+lennart.passengers+" reisijat");
    System.out.println("Lennukis on vabu kohti: "+lennart.free_spots());
  }
}

class aircraft{
  int seats;
  int passengers; 
  int cruiseSpeed; 
  double fuelCapacity; 
  double fuelBurnRate; 

  public double calculateEndurance(){
    double endurance;
    endurance = fuelCapacity / fuelBurnRate;
    return endurance;
  }


  public int free_spots(){
    return seats - passengers;
  }
}
